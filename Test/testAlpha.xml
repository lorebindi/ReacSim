<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level2/version4" level="2" version="4">
  <model id="LotkaVolterra">

    <!-- Definizione delle unitÃ  -->
    <listOfUnitDefinitions>
      <unitDefinition id="per_day">
        <listOfUnits>
          <unit kind="second" multiplier="86400" scale="0" exponent="1"/>
        </listOfUnits>
      </unitDefinition>

      <unitDefinition id="per_item_day">
        <listOfUnits>
          <unit kind="item" exponent="-1"/>
          <unit kind="second" multiplier="86400" exponent="-1"/>
        </listOfUnits>
      </unitDefinition>
    </listOfUnitDefinitions>

    <listOfCompartments>
      <compartment id="cell" size="1" constant="true" />
    </listOfCompartments>

    <!-- Specie -->
    <listOfSpecies>
      <species id="Prey" compartment="cell" initialAmount="20"
               boundaryCondition="false" constant="false" hasOnlySubstanceUnits="true"
               substanceUnits="item"/>
      <!--<species id="Predator" compartment="cell" initialAmount="4"
               boundaryCondition="false" constant="false" hasOnlySubstanceUnits="false"
               substanceUnits="item"/>-->
    </listOfSpecies>

    <!-- Parametri -->
    <listOfParameters>
      <!--<parameter id="alpha" value="0.001" constant="false" units="per_day"/>--> <!-- tasso di crescita delle prede -->
      <parameter id="time_delay" value="0.10" constant="true" units="per_day"/> <!--  -->
    </listOfParameters>

     <!-- Reazioni -->
    <listOfReactions>
      <!-- Reazione 1:   2 Prey -> 3 Prey  (crescita della preda) -->
      <reaction id="PreyGrowth" reversible="false">
        <listOfReactants>
          <speciesReference species="Prey" stoichiometry="2"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Prey" stoichiometry="3"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci>alpha</ci>
              <apply>
                <power/>
                <ci>Prey</ci>
                <cn type="real">2</cn>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>

    </listOfReactions>
  </model>
</sbml>
