<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level2/version4" level="2" version="4">
  <model id="LotkaVolterra">

    <!-- Definizione delle unità -->
    <listOfUnitDefinitions>
      <unitDefinition id="per_second">
        <listOfUnits>
          <unit kind="second" exponent="-1"/>
        </listOfUnits>
      </unitDefinition>

      <unitDefinition id="per_item_second">
        <listOfUnits>
          <unit kind="item" exponent="-1"/>
          <unit kind="second" exponent="-1"/>
        </listOfUnits>
      </unitDefinition>

    </listOfUnitDefinitions>

    <listOfCompartments>
      <compartment id="cell" size="1" constant="true" />
    </listOfCompartments>

    <!-- Specie -->
    <listOfSpecies>
      <species id="Prey" compartment="cell" initialAmount="2"
               boundaryCondition="false" constant="false" hasOnlySubstanceUnits="false"
               substanceUnits="item"/>
      <species id="Predator" compartment="cell" initialAmount="1"
               boundaryCondition="false" constant="false" hasOnlySubstanceUnits="false"
               substanceUnits="item"/>
    </listOfSpecies>

    <!-- Parametri -->
    <listOfParameters>
      <!-- <parameter id="alpha" value="2" constant="false" units="per_second"/> --> <!-- tasso di crescita delle prede -->
      <parameter id="beta" value="0.001" constant="false" units="per_item_second"/> <!-- tasso di successo della caccia -->
      <parameter id="gamma" value="0.001" constant="true" units="per_second"/> <!-- tasso di mortalità naturale -->
      <parameter id="time_delay" value="0.10" constant="true" units="per_second"/> <!--  -->
    </listOfParameters>

     <!-- Reazioni -->
    <listOfReactions>

      <!-- Reazione 1:   2 Prey -> 3 Prey  (crescita della preda) -->
      <reaction id="PreyGrowth" reversible="false">
        <listOfReactants>
          <speciesReference species="Prey" stoichiometry="1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Prey" stoichiometry="2"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci>alpha</ci>
              <apply>
                <power/>
                <ci>Prey</ci>
                <cn type="real">1</cn>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>

